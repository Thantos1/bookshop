<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.bookshop.domain">

	<class name="CartMerchandise" table="CART_MERCHANDISE">
        <id name="id">
        <generator class="native"></generator>
        </id>
       <property name="cart" column="cart_id" type="java.lang.Integer"></property>
       <property name="merchandise" column="merchandise_id" type="java.lang.Integer"></property>
       <property name="number" type="java.lang.Integer" column="MERNUMBER"></property>
       <property name="price" type="java.lang.Double"></property>
       <property name="money" type="java.lang.Double"></property>
       <property name="commentStatus" type="java.lang.Integer"></property>       
    </class>

</hibernate-mapping>